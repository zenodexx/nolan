{
  "name": "btch-downloader",
  "version": "5.0.9",
  "description": "A lightweight TypeScript/JavaScript library for downloading media from social media platforms",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "SECURITY.md"
  ],
  "scripts": {
    "build": "tsc",
    "pub": "npm run build && npm publish --access public",
    "test": "npm run build && ts-node test/fetcherTest.ts",
    "docs": "npx jsdoc -c jsdoc.conf.json",
    "prepare": "npm run build"
  },
  "keywords": [
    "AIO",
    "Instagram",
    "TikTok",
    "Facebook",
    "YouTube",
    "Twitter",
    "X",
    "MediaFire",
    "Capcut",
    "Gdrive",
    "Pinterest",
    "Douyin",
    "Xiaohongshu",
    "Rednote",
    "SnackVideo",
    "media-downloader",
    "social-media"
  ],
  "author": "Tio",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "clean-jsdoc-theme": "^4.3.0",
    "jsdoc": "^4.0.4",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.2",
    "undici-types": "^7.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hostinger-bot/btch-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/hostinger-bot/btch-downloader/issues"
  },
  "homepage": "https://github.com/hostinger-bot/btch-downloader#readme",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "engines": {
    "node": ">=20.18.1"
  }
}
